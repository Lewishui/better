<% content_for :page_title do %>
  <%= link_to "会员管理", admin_users_url %> /
   会员详情
<% end %>
<%= render 'admin/shared/user_tabs', current: "pp" %>
<%= better_form_for(@user, :url=>password_protect_admin_user_path(@user), method: :patch ) do |f| %>
  <div class="form-inputs">
    <%= f.input :pp_question %>
    <%= f.input :pp_answer %>
  </div>
  <div class="form-actions">
    <%= f.button :submit, "保存" %>
  </div>
<% end %>
