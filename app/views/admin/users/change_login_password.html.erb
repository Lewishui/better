<% content_for :page_title do %>
  <%= link_to "会员管理", admin_users_url %> /
   会员详情
<% end %>
<%= render 'admin/shared/user_tabs', current: "lp" %>

<%= better_form_for(@user, :url =>change_login_password_admin_user_path(@user), method: :patch ) do |f| %>
  <div class="form-inputs">
    <%= f.input :password %>
    <%= f.input :password_confirmation %>
  </div>
  <div class="form-actions">
    <%= f.button :submit, "保存" %>
  </div>
<% end %>
