<table>
  <thead>
    <tr>
      <th>用户名/ID</th>
      <th>注册时间</th>
      <th>用户类型</th>
      <th>状态</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.nickname %></td>
        <td><%= user.display_created_at %></td>
        <td><%= user.type %></td>
        <td><%= user.state %></td>
      </tr>
    <% end %>
  </tbody>

</table>
