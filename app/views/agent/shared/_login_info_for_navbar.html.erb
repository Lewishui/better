
<% if broker_signed_in? %>
  <%= link_to 'Account', agent_broker_index_path, class: 'nav-link' %>
  <%= link_to 'Sign out', destroy_broker_session_path, :method=>'delete', class: 'nav-link' %>
<% else %>
  <%= link_to 'Sign in', new_broker_session_path, class: 'nav-link' %>
  <%= link_to 'Sign up', new_broker_registration_path, class: 'nav-link' %>
<% end %>
<% if broker_signed_in? %>
  <% if current_broker.try(:admin?) %>
    <%= link_to 'brokers', brokers_path, class: 'nav-link' %>
  <% end %>
<% end %>
