<%= simple_form_for(@drawing) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user_bank %>
    <%= f.input :number %>
    <%= f.input :amount %>
    <%= f.input :state %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
